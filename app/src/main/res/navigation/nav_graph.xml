<?xml version="1.0" encoding="utf-8"?>
<navigation
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools" android:id="@+id/nav_graph"
        app:startDestination="@id/nickNameFragment">


    <fragment android:id="@+id/mainFragment"
              android:name="com.wlswnwns.chosung_android.main.MainFragment"
              android:label="layout_main"
              tools:layout="@layout/layout_main">
        <argument android:name="strNickName" app:argType="string"/>
        <action android:id="@+id/action_mainFragment_to_hunminGameFragment" app:destination="@id/hunminGameFragment"/>
    </fragment>

    <fragment android:id="@+id/nickNameFragment"
              android:name="com.wlswnwns.chosung_android.nickname.NickNameFragment"
              android:label="layout_nickname">
        <action android:id="@+id/action_nickNameFragment_to_mainFragment" app:destination="@id/mainFragment"/>
    </fragment>
    <fragment android:id="@+id/hunminGameFragment"
              android:name="com.wlswnwns.chosung_android.hunminGame.HunminGameFragment"
              android:label="layout_hunmingame"
              tools:layout="@layout/layout_hunmingame">
        <action android:id="@+id/action_hunminGameFragment_to_mainFragment"
        />
    </fragment>


</navigation>